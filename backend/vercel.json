{
    "version": 2,
    "builds": [
      {
        "src": "backend/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/src/**/*",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/src/$1"
      }
    ]
  }
  
